<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.wm.cs.cs301.benzhang.amazebybenzhang"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="edu.wm.cs.cs301.benzhang.amazebybenzhang.AMazeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.wm.cs.cs301.benzhang.amazebybenzhang.GeneratingActivity"
            android:label="@string/title_activity_generating"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="edu.wm.cs.cs301.benzhang.amazebybenzhang.PlayActivity"
            android:parentActivityName="edu.wm.cs.cs301.benzhang.amazebybenzhang.AMazeActivity"
            android:theme="@style/AppTheme.NoActionBar" >

            <!-- The meta-data element is needed for versions lower than 4.1 -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AMazeActivity" />
        </activity>
        <activity android:name="edu.wm.cs.cs301.benzhang.amazebybenzhang.FinishActivity" />
    </application>

</manifest>